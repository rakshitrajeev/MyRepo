import json

with open('fullFledgedTM.json', encoding="utf-8", errors='ignore') as d:
    events_dict = json.load(d)
listOfEventNames = []
for eachEvent in events_dict['_embedded']['events']:
    eventName = eachEvent['name']
    #print(eventName)
    listOfEventNames.append(eventName)
    
print(listOfEventNames)


with open('fullFledgedTwitter.json', encoding="utf-8", errors='ignore') as d:
    twitter_dict = json.load(d)
listOfTwittNames = []
for eachTwitt in twitter_dict['tweets']:
    twittText = eachTwitt['full_text']
    #print(twittText)
    listOfTwittNames.append(twittText)
    
print(listOfTwittNames)
    